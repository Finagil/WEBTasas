          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="VWBloqueoTableAdapter" GeneratorDataComponentClassName="VWBloqueoTableAdapter" Name="VWBloqueo" UserDataComponentName="VWBloqueoTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ProductionConnectionString (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Vw_Anexos.AnexoCon, Vw_Anexos.Descr AS Cliente, { fn UCASE(GEN_Bloqueo_Tasas.ComentarioPromo) } AS ComentarioPromo, CASE WHEN aNEXOS.Tipta = '7' THEN CONVERT(varchar, Tasapol) 
                         ELSE 'TIIE + ' + CONVERT(varchar, Tasapol) END AS Tasa_Politica, CASE WHEN aNEXOS.Tipta = '7' THEN CONVERT(varchar, TasaSol) ELSE 'TIIE + ' + CONVERT(varchar, TasaSol) END AS Tasa_Solicitada, 
                         Vw_Anexos.TipoCredito, Vw_Anexos.MontoFinanciado, Vw_Anexos.FondoReserva, Anexos.Plazo, CASE WHEN aNEXOS.Tipta = '7' THEN 'FIJA' ELSE 'VARIABLE' END AS Tipo_Tasa, Anexos.Porco AS PorcentajeComision, 
                         Anexos.Porop AS RecidualOP, Anexos.Taspen AS Penalizacion, Anexos.RD AS RentasenDeposito, Anexos.DG AS DespoitoenGarantia, GEN_Bloqueo_Tasas.id, 
                         GEN_Bloqueo_Tasas.TasaPol - GEN_Bloqueo_Tasas.TasaSol AS Diferencia, GEN_Bloqueo_Tasas.ComentarioRiesgos, GEN_Bloqueo_Tasas.AutorizadoRI, GEN_Bloqueo_Tasas.AutorizadoDG, 
                         GEN_Bloqueo_Tasas.FirmaSubPromo, Vw_AnexoPlazoMeses.Plazo AS PlazoEnMeses
FROM            GEN_Bloqueo_Tasas INNER JOIN
                         Vw_Anexos ON SUBSTRING(GEN_Bloqueo_Tasas.Anexo, 2, 9) = SUBSTRING(Vw_Anexos.Anexo, 2, 9) INNER JOIN
                         Anexos ON SUBSTRING(GEN_Bloqueo_Tasas.Anexo, 2, 9) = SUBSTRING(Anexos.Anexo, 2, 9) INNER JOIN
                         Vw_AnexoPlazoMeses ON Anexos.Anexo = Vw_AnexoPlazoMeses.Anexo
WHERE        (GEN_Bloqueo_Tasas.id = @ID)</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="ID" ColumnName="id" DataSourceName="Production.dbo.GEN_Bloqueo_Tasas" DataTypeServer="numeric" DbType="Decimal" Direction="Input" ParameterName="@ID" Precision="18" ProviderType="Decimal" Scale="0" Size="9" SourceColumn="id" SourceColumnNullMapping="false" SourceVersion="Current" />
